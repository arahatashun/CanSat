#Makefile_flight

LIBS = -lwiringPi -lm -lgps
objs0 = integration_knd.o motor.o mitibiki.o compass.o acclgyro.c ring_buffer.o

knd.out: $(objs0)
        gcc -g -Wall -O2 knd.out $(Objs0) $(LIBS)
integration_knd.o: integration_knd.c
        gcc -c intagration_knd.c
mitibiki.o: mitibiki.c
        gcc -c mitibiki.c
compass.o: compass.c
        gcc -c compass.c
acclgyro.o: acclgyro.c
        gcc -c acclgyro.c
motor.o: motor.c
        gcc -c motor.c
ring_buffer.o:ring_buffer.c
        gcc -c ring_buffer.c
