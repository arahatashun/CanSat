#Makefile_knd2(2-dim)

LIBS = -lwiringPi -lm -lgps
objs0 = integration_knd.o motor.o mitibiki.o compass.o ring_buffer.o

knd2.out: $(objs0)
	gcc -g -Wall -O2 knd2.out $(Objs0) $(LIBS)
integration_knd2.o: integration_knd2.c
	gcc -c integration_knd2.c
mitibiki.o: mitibiki.c
	gcc -c mitibiki.c
compass.o: compass.c
	gcc -c compass.c
acclgyro.o: acclgyro.c
	gcc -c acclgyro.c
motor.o: motor.c
	gcc -c motor.c
ring_buffer.o:ring_buffer.c
	gcc -c ring_buffer.c
