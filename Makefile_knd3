#Makefile_knd2

LIBS = -lwiringPi -lm -lgps
objs0 = integration_knd3.o motor.o compass.o ring_buffer.o mitibiki.o 

integration_knd3.out: $(objs0)
	gcc -g -Wall -O2 -o integration_knd3.out $(objs0) $(LIBS)
integration_knd3.o: integration_knd3.c
	gcc -c -Wall integration_knd3.c
motor.o: motor.c
	gcc -c motor.c
compass.o: compass.c
	gcc -c compass.c
ring_buffer.o: ring_buffer.c
	gcc -c ring_buffer.c
mitibiki.o: mitibiki.c
	gcc -c mitibiki.c
